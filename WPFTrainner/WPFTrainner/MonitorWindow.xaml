<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PVZWPFTrainner"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:darkstyle="clr-namespace:DarkStyle;assembly=DarkStyle"
    x:Class="MonitorWindow"
        mc:Ignorable="d"
        Title="监视窗口" Height="470" Width="400" MouseDown="Window_MouseDown" Closed="Window_Closed"
        WindowStyle="None" ResizeMode="NoResize" Loaded="Window_Loaded" PreviewMouseWheel="Window_PreviewMouseWheel"
        Background="#FF252526" WindowStartupLocation="CenterScreen">
    <Canvas>
        <Rectangle Stroke="Crimson" StrokeThickness="2" Height="470" Width="400"/>
        <TextBlock Foreground="White" Text="监视窗口" FontWeight="Bold" FontSize="20"  Canvas.Left="10" Canvas.Top="10">
            <TextBlock.Resources>
                <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                    <sys:String>监视窗口</sys:String>
                    <sys:String>Monitor</sys:String>
                </x:Array>
            </TextBlock.Resources>
        </TextBlock>
        <darkstyle:DarkCheckBox x:Name="CBHpTrack" Content="启动僵尸血量显示" ToolTipService.ShowDuration="20000"
                       Canvas.Left="105" Canvas.Top="16" Click="CBHpTrack_Click">
            <darkstyle:DarkCheckBox.Resources>
                <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                    <sys:String>启动僵尸血量显示</sys:String>
                    <sys:String>ShowZombieHp</sys:String>
                </x:Array>
            </darkstyle:DarkCheckBox.Resources>
        </darkstyle:DarkCheckBox>
        <TextBox x:Name="TBColor" Canvas.Left="230" Foreground="White" Canvas.Top="14" Width="80" Background="#FF252526"
                       PreviewKeyDown="TBColor_PreviewKeyDown"  Text="White"/>
        <darkstyle:CloseButton Canvas.Left="360" Canvas.Top="10"/>
        <darkstyle:MinimizeButton Canvas.Left="320" Canvas.Top="10"/>
        <TextBlock  Canvas.Left="10" Canvas.Top="35" Foreground="Crimson" Text="-------------------------------------------------------------------------"/>
        <TabControl x:Name="TCMain" Width="380" Canvas.Left="10" Canvas.Top="60" Height="390" PreviewKeyDown="TCMain_PreviewKeyDown"
                        Background="#FF252526" BorderThickness="1" SelectionChanged="TCMain_SelectionChanged">
            <TabItem Foreground="White" Header="鼠标指针" TabIndex="0"  Background="#FF252526" Style="{DynamicResource TabItemStyle1}">
                <TabItem.Resources>
                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                        <sys:String>鼠标指针</sys:String>
                        <sys:String>Mouse</sys:String>
                    </x:Array>
                </TabItem.Resources>
                <Canvas Background="#FF252525">
                    <TextBlock Text="是否在游戏内"  Canvas.Left="23" Canvas.Top="82" >
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>是否在游戏内</sys:String>
                                <sys:String>InGameArea</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <TextBlock x:Name="TBMouseInGameArea" Text="否"  Canvas.Left="153" Canvas.Top="82"/>
                    <TextBlock Text="横坐标"  Canvas.Left="23" Canvas.Top="122">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>横坐标</sys:String>
                                <sys:String>X</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <TextBlock x:Name="TBMouseX" Text="0"  Canvas.Left="153" Canvas.Top="122"/>
                    <TextBlock Text="纵坐标"  Canvas.Left="23" Canvas.Top="162">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>纵坐标</sys:String>
                                <sys:String>Y</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <TextBlock x:Name="TBMouseY" Text="0"  Canvas.Left="153" Canvas.Top="162"/>
                    <TextBlock Text="点击状态"  Canvas.Left="23" Canvas.Top="202">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>点击状态</sys:String>
                                <sys:String>ClickState</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <TextBlock x:Name="TBMouseStateLeft" Text="左键"  Canvas.Left="153" Canvas.Top="202">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>左键</sys:String>
                                <sys:String>Left</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <TextBlock x:Name="TBMouseStateMid" Text="中键"  Canvas.Left="193" Canvas.Top="202">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>中键</sys:String>
                                <sys:String>Mid</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <TextBlock x:Name="TBMouseStateRight" Text="右键"  Canvas.Left="233" Canvas.Top="202">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>右键</sys:String>
                                <sys:String>Right</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <TextBlock Text="X坐标" TextAlignment="Center" Canvas.Left="23" Canvas.Top="244" Width="40">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>X坐标</sys:String>
                                <sys:String>X</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <darkstyle:DarkTextBox x:Name="TBoxMouseX" BorderThickness="1"
                                         Canvas.Left="63" Canvas.Top="243" PreviewKeyDown="TBoxMouseX_PreviewKeyDown"/>
                    <TextBlock Text="Y坐标"  Canvas.Left="153" Canvas.Top="244" Width="40" TextAlignment="Center">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>Y坐标</sys:String>
                                <sys:String>Y</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <darkstyle:DarkTextBox x:Name="TBoxMouseY" BorderThickness="1"
                                         Canvas.Left="193" Canvas.Top="243" PreviewKeyDown="TBoxMouseX_PreviewKeyDown"/>
                    <darkstyle:DarkButton x:Name="BtnWMClick" Content="点击坐标" Width="70"
                                Canvas.Left="287" Canvas.Top="242" Click="BtnWMClick_Click">
                        <darkstyle:DarkButton.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>点击坐标</sys:String>
                                <sys:String>DoClick</sys:String>
                            </x:Array>
                        </darkstyle:DarkButton.Resources>
                    </darkstyle:DarkButton>
                    <TextBlock Text="所在行"  Canvas.Left="194" Canvas.Top="122">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>所在行</sys:String>
                                <sys:String>Row</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <TextBlock x:Name="TBMouseRow" Text="0"  Canvas.Left="323" Canvas.Top="122"/>
                    <TextBlock Text="所在列"  Canvas.Left="194" Canvas.Top="162">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>所在列</sys:String>
                                <sys:String>Column</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <TextBlock x:Name="TBMouseColumn" Text="0"  Canvas.Left="323" Canvas.Top="162"/>
                </Canvas>
            </TabItem>
            <TabItem Foreground="White" Header="僵尸" TabIndex="1"  Background="#FF252526" Style="{DynamicResource TabItemStyle1}">
                <TabItem.Resources>
                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                        <sys:String>僵尸</sys:String>
                        <sys:String>Zombies</sys:String>
                    </x:Array>
                </TabItem.Resources>
                <Canvas Background="#FF252525">
                    <ListBox x:Name="LBZombies" Width="100" Height="326" Canvas.Left="10" Canvas.Top="10" SelectionChanged="LBZombies_SelectionChanged"
                                 Background="#FF252526" ScrollViewer.CanContentScroll="True" Style="{DynamicResource ListBoxStyle1}"/>
                    <TextBlock x:Name="TBZombieType" Text="类型"  Canvas.Left="120" Canvas.Top="10">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>类型</sys:String>
                                <sys:String>Type</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <TextBlock x:Name="TBZombieId" Text="id="  Canvas.Left="245" Canvas.Top="10" MouseDown="TBZombieId_MouseDown">
                        <TextBlock.ToolTip>
                            <local:MyToolTip Content="点击复制">
                                <ToolTip.Resources>
                                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                        <sys:String>点击复制</sys:String>
                                        <sys:String>Click to copy</sys:String>
                                    </x:Array>
                                </ToolTip.Resources>
                            </local:MyToolTip>
                        </TextBlock.ToolTip>
                    </TextBlock>
                    <TextBlock Text="X坐标"  Canvas.Left="120" Canvas.Top="35" Width="40" TextAlignment="Center">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>X坐标</sys:String>
                                <sys:String>X</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <darkstyle:DarkTextBox x:Name="TBZombieX" Text="0" Canvas.Left="160" Canvas.Top="34" Width="75"
                                 PreviewKeyDown="TBZombieX_PreviewKeyDown" BorderThickness="1"/>
                    <TextBlock Text="Y坐标"  Canvas.Left="240" Canvas.Top="35" Width="48" TextAlignment="Center">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>Y坐标</sys:String>
                                <sys:String>Y</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <darkstyle:DarkTextBox x:Name="TBZombieY" Text="0" Canvas.Left="289" Canvas.Top="34" Width="75"
                                 PreviewKeyDown="TBZombieY_PreviewKeyDown" BorderThickness="1"/>
                    <TextBlock Text="所在行" Canvas.Left="120" Canvas.Top="66" Width="40" TextAlignment="Center">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>所在行</sys:String>
                                <sys:String>Row</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <darkstyle:Numericupdown x:Name="NudZombieRow" Value="1" Canvas.Left="160" MaxValue="6" MinValue="1" Canvas.Top="60" Width="80" ValueChanged="NudZombieRow_ValueChanged"/>
                    <TextBlock Text="状态"  Canvas.Left="240" Canvas.Top="66" Width="48" TextAlignment="Center">
                        <TextBlock.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>状态</sys:String>
                                <sys:String>State</sys:String>
                            </x:Array>
                        </TextBlock.Resources>
                    </TextBlock>
                    <darkstyle:DarkTextBox x:Name="TBZombieState" Text="0" Canvas.Left="289" Canvas.Top="66" Width="75"
                                 PreviewKeyDown="TBZombieState_PreviewKeyDown" BorderThickness="1"/>
                    <GroupBox BorderThickness="0.5" Header="血量" Canvas.Left="120" Canvas.Top="92" Height="101" Width="130">
                        <GroupBox.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>血量</sys:String>
                                <sys:String>Hp</sys:String>
                            </x:Array>
                        </GroupBox.Resources>
                        <Canvas>
                            <TextBlock Text="本体" Width="38" TextAlignment="Center" Canvas.Top="10">
                                <TextBlock.Resources>
                                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                        <sys:String>本体</sys:String>
                                        <sys:String>Body</sys:String>
                                    </x:Array>
                                </TextBlock.Resources>
                            </TextBlock>
                            <darkstyle:DarkTextBox x:Name="TBZombieBodyHp" Text="0" Canvas.Left="38" Canvas.Top="8" Width="75"
                                    PreviewKeyDown="TBZombieBodyHp_PreviewKeyDown" BorderThickness="1"/>
                            <TextBlock Text="饰品1" Width="38" TextAlignment="Center" Canvas.Top="35">
                                <TextBlock.Resources>
                                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                        <sys:String>饰品1</sys:String>
                                        <sys:String>Hat</sys:String>
                                    </x:Array>
                                </TextBlock.Resources>
                            </TextBlock>
                            <darkstyle:DarkTextBox x:Name="TBZombieA1Hp" Text="0" Canvas.Left="38" Canvas.Top="34" Width="74" 
                                    PreviewKeyDown="TBZombieA1Hp_PreviewKeyDown" BorderThickness="1"/>
                            <TextBlock Text="饰品2" Width="38" TextAlignment="Center" Canvas.Top="60">
                                <TextBlock.Resources>
                                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                        <sys:String>饰品2</sys:String>
                                        <sys:String>Shield</sys:String>
                                    </x:Array>
                                </TextBlock.Resources>
                            </TextBlock>
                            <darkstyle:DarkTextBox x:Name="TBZombieA2Hp" Text="0" Canvas.Left="38" Canvas.Top="59" Width="74"
                                    PreviewKeyDown="TBZombieA2Hp_PreviewKeyDown" BorderThickness="1"/>
                        </Canvas>
                    </GroupBox>
                    <GroupBox BorderThickness="0.5" Header="属性" Canvas.Left="262" Canvas.Top="92" Height="158" Width="100">
                        <GroupBox.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>属性</sys:String>
                                <sys:String>Property</sys:String>
                            </x:Array>
                        </GroupBox.Resources>
                        <Canvas>
                            <darkstyle:DarkCheckBox x:Name="CBZombieVisible" Content="隐形"  Canvas.Left="2" Canvas.Top="5" Click="CBZombieVisible_Click">
                                <darkstyle:DarkCheckBox.Resources>
                                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                        <sys:String>隐形</sys:String>
                                        <sys:String>InVisible</sys:String>
                                    </x:Array>
                                </darkstyle:DarkCheckBox.Resources>
                            </darkstyle:DarkCheckBox>
                            <darkstyle:DarkCheckBox x:Name="CBZombieHypnotized" Content="魅惑"  Canvas.Left="2" Canvas.Top="27" Click="CBZombieHypnotized_Click">
                                <darkstyle:DarkCheckBox.Resources>
                                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                        <sys:String>魅惑</sys:String>
                                        <sys:String>Hypnotized</sys:String>
                                    </x:Array>
                                </darkstyle:DarkCheckBox.Resources>
                            </darkstyle:DarkCheckBox>
                            <darkstyle:DarkCheckBox x:Name="CBZombieBlowaway" Content="吹飞"  Canvas.Left="2" Canvas.Top="49" Click="CBZombieBlowaway_Click">
                                <darkstyle:DarkCheckBox.Resources>
                                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                        <sys:String>吹飞</sys:String>
                                        <sys:String>Blowaway</sys:String>
                                    </x:Array>
                                </darkstyle:DarkCheckBox.Resources>
                            </darkstyle:DarkCheckBox>
                            <darkstyle:DarkCheckBox x:Name="CBZombieDying" Content="低于临界"  Canvas.Left="2" Canvas.Top="71" Click="CBZombieDying_Click">
                                <darkstyle:DarkCheckBox.Resources>
                                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                        <sys:String>低于临界</sys:String>
                                        <sys:String>Dying</sys:String>
                                    </x:Array>
                                </darkstyle:DarkCheckBox.Resources>
                            </darkstyle:DarkCheckBox>
                            <darkstyle:DarkCheckBox  x:Name="CBZombieGarlicBited" Content="咬到大蒜"  Canvas.Left="2" Canvas.Top="93"  Click="CBZombieGarlicBited_Click">
                                <darkstyle:DarkCheckBox.Resources>
                                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                        <sys:String>咬到大蒜</sys:String>
                                        <sys:String>Garlic</sys:String>
                                    </x:Array>
                                </darkstyle:DarkCheckBox.Resources>
                            </darkstyle:DarkCheckBox>
                            <darkstyle:DarkCheckBox x:Name="CBZombieExist" Content="存在"  Canvas.Left="2" Canvas.Top="115" Click="CBZombieExist_Click">
                                <darkstyle:DarkCheckBox.Resources>
                                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                        <sys:String>存在</sys:String>
                                        <sys:String>Exist</sys:String>
                                    </x:Array>
                                </darkstyle:DarkCheckBox.Resources>
                            </darkstyle:DarkCheckBox>
                        </Canvas>
                    </GroupBox>
                    <GroupBox BorderThickness="0.5" Header="倒计时" Canvas.Left="120" Canvas.Top="198" Height="100" Width="130" >
                        <GroupBox.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>倒计时</sys:String>
                                <sys:String>Countdown</sys:String>
                            </x:Array>
                        </GroupBox.Resources>
                        <Canvas>
                            <TextBlock Text="减速" Width="36" TextAlignment="Center" Canvas.Top="7">
                                <TextBlock.Resources>
                                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                        <sys:String>减速</sys:String>
                                        <sys:String>Cold</sys:String>
                                    </x:Array>
                                </TextBlock.Resources>
                            </TextBlock>
                            <local:MySlider x:Name="SZombieDecelerate" Canvas.Left="38" Canvas.Top="6" ValueChanged="SZombieDecelerate_ValueChanged"/>
                            <TextBlock Text="黄油" Width="36" TextAlignment="Center" Canvas.Top="32">
                                <TextBlock.Resources>
                                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                        <sys:String>黄油</sys:String>
                                        <sys:String>Butter</sys:String>
                                    </x:Array>
                                </TextBlock.Resources>
                            </TextBlock>
                            <local:MySlider x:Name="SZombieFixed" Canvas.Left="38" Canvas.Top="31" ValueChanged="SZombieFixed_ValueChanged"/>
                            <TextBlock Text="冻结" Width="36" TextAlignment="Center" Canvas.Top="57">
                                <TextBlock.Resources>
                                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                        <sys:String>冻结</sys:String>
                                        <sys:String>Froze</sys:String>
                                    </x:Array>
                                </TextBlock.Resources>
                            </TextBlock>
                            <local:MySlider x:Name="SZombieFrozen" Canvas.Left="38" Canvas.Top="56" ValueChanged="SZombieFrozen_ValueChanged"/>
                        </Canvas>
                    </GroupBox>
                    <darkstyle:DarkButton x:Name="BtnZombieButter" Content="黄油固定" Width="60" Canvas.Left="120" Canvas.Top="313" Click="BtnZombieButter_Click">
                        <darkstyle:DarkButton.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>黄油固定</sys:String>
                                <sys:String>Butter</sys:String>
                            </x:Array>
                        </darkstyle:DarkButton.Resources>
                    </darkstyle:DarkButton>
                    <darkstyle:DarkButton x:Name="BtnZombieBlast" Content="爆炸伤害" Width="60" Canvas.Left="190" Canvas.Top="313" Click="BtnZombieBlast_Click">
                        <darkstyle:DarkButton.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>爆炸伤害</sys:String>
                                <sys:String>Blast</sys:String>
                            </x:Array>
                        </darkstyle:DarkButton.Resources>
                    </darkstyle:DarkButton>
                    <darkstyle:DarkTextBox x:Name="TBZombieDamage" Text="0" Canvas.Left="275" Canvas.Top="255" Width="75" BorderThickness="1"/>
                    <darkstyle:DarkComboBox x:Name="CBZombieDamageType"
                            Canvas.Left="275" Canvas.Top="283" Width="80" >
                        <darkstyle:DarkComboBoxItem Content="正面伤害">
                            <darkstyle:DarkComboBoxItem.Resources>
                                <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                    <sys:String>正面伤害</sys:String>
                                    <sys:String>Direct</sys:String>
                                </x:Array>
                            </darkstyle:DarkComboBoxItem.Resources>
                        </darkstyle:DarkComboBoxItem>
                        <darkstyle:DarkComboBoxItem Content="非正面伤害">
                            <darkstyle:DarkComboBoxItem.Resources>
                                <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                    <sys:String>非正面伤害</sys:String>
                                    <sys:String>NotDirect</sys:String>
                                </x:Array>
                            </darkstyle:DarkComboBoxItem.Resources>
                        </darkstyle:DarkComboBoxItem>
                        <darkstyle:DarkComboBoxItem Content="溅射伤害">
                            <darkstyle:DarkComboBoxItem.Resources>
                                <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                    <sys:String>溅射伤害</sys:String>
                                    <sys:String>Sputter</sys:String>
                                </x:Array>
                            </darkstyle:DarkComboBoxItem.Resources>
                        </darkstyle:DarkComboBoxItem>
                        <darkstyle:DarkComboBoxItem Content="正面减速">
                            <darkstyle:DarkComboBoxItem.Resources>
                                <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                    <sys:String>正面减速</sys:String>
                                    <sys:String>DirectIce</sys:String>
                                </x:Array>
                            </darkstyle:DarkComboBoxItem.Resources>
                        </darkstyle:DarkComboBoxItem>
                        <darkstyle:DarkComboBoxItem Content="非正面减速">
                            <darkstyle:DarkComboBoxItem.Resources>
                                <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                    <sys:String>非正面减速</sys:String>
                                    <sys:String>NotDirectIce</sys:String>
                                </x:Array>
                            </darkstyle:DarkComboBoxItem.Resources>
                        </darkstyle:DarkComboBoxItem>
                        <darkstyle:DarkComboBoxItem Content="溅射减速">
                            <darkstyle:DarkComboBoxItem.Resources>
                                <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                    <sys:String>溅射减速</sys:String>
                                    <sys:String>SputterIce</sys:String>
                                </x:Array>
                            </darkstyle:DarkComboBoxItem.Resources>
                        </darkstyle:DarkComboBoxItem>
                    </darkstyle:DarkComboBox>
                    <darkstyle:DarkButton x:Name="BtnZombieHit" Content="伤害" Width="80"
                            Canvas.Left="275" Canvas.Top="313" Click="BtnZombieHit_Click">
                        <Button.Resources>
                            <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                                <sys:String>伤害</sys:String>
                                <sys:String>Damage</sys:String>
                            </x:Array>
                        </Button.Resources>
                    </darkstyle:DarkButton>
                </Canvas>
            </TabItem>
            <TabItem Foreground="White" Header="植物" TabIndex="2"  Background="#FF252526" Style="{DynamicResource TabItemStyle1}">
                <TabItem.Resources>
                    <x:Array x:Key="Lang" Type="{x:Type sys:String}">
                        <sys:String>植物</sys:String>
                        <sys:String>Plants</sys:String>
                    </x:Array>
